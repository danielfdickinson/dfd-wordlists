{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"allowCompoundWords": false,
	"dictionaries": [
		"project-words",
		"project-words-fr",
		"tech-words",
		"hugo-words",
		"dfd-words",
		"dfd-words-fr"
	],
	"dictionaryDefinitions": [
		{
			"addWords": true,
			"locale": "en,en-ca,en-gb,en-us",
			"name": "project-words",
			"path": "./tests/config/words-project.txt"
		},
		{
			"addWords": true,
			"locale": "fr,fr-ca",
			"name": "project-words-fr",
			"path": "./tests/config/words-fr-project.txt"
		},
		{
			"addWords": true,
			"locale": "en,en-ca,en-gb,en-us",
			"name": "tech-words",
			"path": "./words-tech.txt"
		},
		{
			"addWords": true,
			"locale": "en,en-ca,en-gb,en-us",
			"name": "hugo-words",
			"path": "./words-hugo.txt"
		},
		{
			"addWords": true,
			"locale": "en,en-ca,en-gb,en-us",
			"name": "dfd-words",
			"path": "./words-dfd.txt"
		},
		{
			"addWords": true,
			"locale": "fr,fr-ca",
			"name": "dfd-words-fr",
			"path": "./words-fr-dfd.txt"
		}
	],
	"globRoot": "../..",
	"ignorePaths": [
		"node_modules",
		"./tests/config/cspell.json",
		"./tests/config/words-*.txt",
		"./words-*.txt"
	],
	"ignoreWords": [],
	"language": "en,en-ca,en-gb",
	"version": "0.2"
}
